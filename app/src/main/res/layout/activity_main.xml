<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/intro_back"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/intro_back"
    android:clipToOutline="true">
    <!-- Asset 1 1 -->
    <View
        android:id="@+id/intro_B"
        android:layout_width="42.77dp"
        android:layout_height="47.7dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="92.43dp"
        android:layout_marginTop="242dp"
        android:background="@drawable/intro_B"
        android:elevation="4dp" />
    <!-- angu. -->
    <TextView
        android:id="@+id/intro_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="123.57dp"
        android:layout_marginTop="249dp"
        android:fontFamily="@font/title_poppins"
        android:gravity="top"
        android:lineSpacingExtra="0sp"
        android:text="@string/intro_title"
        android:textAppearance="@style/intro_title" />
    <!-- 방구석 리뷰 모음 -->
    <TextView
        android:id="@+id/intro_info"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="92.43dp"
        android:layout_marginTop="306.34dp"
        android:fontFamily="@font/spoqa_medium"
        android:gravity="top"
        android:lineSpacingExtra="0sp"
        android:text="@string/intro_info"
        android:textAppearance="@style/intro_info" />

</RelativeLayout>
